<h1>Puppetclasses</h1>
<div class='row'>
  <div class='col-md-3'>
    <table class='table table-bordered table-hover table-striped table-two-pane'>
      <thead>
        <th>Id</th>
        <th>Name</th>
        <th>Module</th>
      </thead>
      <tbody>
      {{#each puppetclass in model}}
      {{!#each puppetclass in model itemController="puppetclass"}}
      <tr>
        <td>
        {{puppetclass.id}}
        </td>
        <td>
        {{puppetclass.name}}
        </td>
        <td>
        {{puppetclass.module_name}}
        </td>
      </tr>
      {{/each}}
      </tbody>
    </table>
  </div>
  {{outlet}}
</div>
